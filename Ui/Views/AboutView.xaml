<UserControl x:Class="Ui.Views.AboutView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Ui.Views"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300"
             xmlns:ViewModels="clr-namespace:Logic.ViewModel;assembly=Logic"
             d:DataContext="{d:DesignInstance Type=ViewModels:AboutViewModel,IsDesignTimeCreatable=True}">
    

    <StackPanel>
        <TextBlock>AboutView</TextBlock>
        <TextBlock Text="{Binding appName}"/>
    </StackPanel>
</UserControl>
